{
  "expo": {
    "name": "MoviKoox",
    "slug": "movikoox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/movikoox_logo.png",
    "scheme": "movikooxapp",
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.movikoox.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/movikoox_logo.png",
        "backgroundColor": "#7A1F33"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/movikoox_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#7A1F33"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "bundleIdentifier": "com.movikoox.app"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "97063dfa-6438-40bb-a42f-8b08ad2600ed"
      }
    }
  }
}
